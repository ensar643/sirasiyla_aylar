void setup() {
Serial.begin(9600);
}
void loop() {
String aylar[11]={"Ocak","Subat","Mart","Nisan","Mayis"};

aylar[5]="Haziran";
aylar[6]="Temmuz"; 
aylar[7]="Agustos";
aylar[8]="Eylül";
aylar[9]="Ekim";
aylar[10]="Kasım";
aylar[11]="Aralık";
int i;
for (i=0;i<12;i++)
{
Serial.println(aylar[i]);

delay(1000);
}
}  
